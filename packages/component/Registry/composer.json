{
    "name": "talav/registry",
    "description": "Registry for different objects",
    "keywords": ["registry", "services"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Talav",
            "email": "support@talav.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "webmozart/assert": "^1.8",
        "zendframework/zend-stdlib": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2",
        "symfony/thanks": "^1.2",
        "talav/coding-standard": "@dev"
    },
    "autoload": {
        "psr-4": {
            "Talav\\Component\\Registry\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Talav\\Component\\Registry\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../../*/*"
        }
    ],
    "scripts": {
        "check-cs": "vendor/bin/ecs check src --ansi",
        "fix-cs": "vendor/bin/ecs check src --fix --ansi"
    }
}
