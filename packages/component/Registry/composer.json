{
    "name": "talav/registry",
    "description": "Registry for different objects",
    "keywords": ["registry", "services"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Talav",
            "email": "support@talav.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "webmozart/assert": "^1.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/thanks": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Talav\\Component\\Registry\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Talav\\Component\\Registry\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../../*/*"
        }
    ]
}
