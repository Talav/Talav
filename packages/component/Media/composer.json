{
    "name": "talav/media",
    "description": "Plan and plan feature functionality for SaaS applications",
    "keywords": ["symfony", "bundle", "subscription", "php"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Talav",
            "email": "support@talav.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "league/flysystem": "^2.1",
        "league/glide": "@dev",
        "symfony/http-foundation": "^5.1",
        "symfony/validator": "^5.1",
        "symfony/mime": "^5.1",
        "talav/resource": "@dev",
        "league/flysystem-memory": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/thanks": "^1.2",
        "webfactory/doctrine-orm-test-infrastructure": "^1.7"
    },
    "autoload": {
        "psr-4": {
            "Talav\\Component\\Media\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Talav\\Component\\Media\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../../*/*"
        }
    ]
}
