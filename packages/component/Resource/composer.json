{
    "name": "talav/resource",
    "description": "Basic resource interfaces for PHP applications.",
    "keywords": ["symfony", "bundle", "user", "php"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Talav",
            "email": "support@talav.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "doctrine/common": "^3.1",
        "gedmo/doctrine-extensions": "^3.1",
        "pagerfanta/pagerfanta": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/thanks": "^1.2",
        "webfactory/doctrine-orm-test-infrastructure": "^1.7"
    },
    "autoload": {
        "psr-4": {
            "Talav\\Component\\Resource\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Talav\\Component\\Resource\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../../*/*"
        }
    ]
}
