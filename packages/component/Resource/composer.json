{
    "name": "talav/resource",
    "description": "Basic resource interfaces for PHP applications.",
    "keywords": ["symfony", "bundle", "user", "php"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Talav",
            "email": "support@talav.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "doctrine/common": "^2.7",
        "gedmo/doctrine-extensions": "^2.4",
        "pagerfanta/pagerfanta": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2",
        "symfony/thanks": "^1.2",
        "talav/coding-standard": "@dev",
        "webfactory/doctrine-orm-test-infrastructure": "^1.7"
    },
    "autoload": {
        "psr-4": {
            "Talav\\Component\\Resource\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Talav\\Component\\Resource\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../../*/*"
        }
    ],
    "scripts": {
        "check-cs": "vendor/bin/ecs check src --ansi",
        "fix-cs": "vendor/bin/ecs check src --fix --ansi",
        "rector": "vendor/bin/rector process --config vendor/talav/coding-standard/rector.yml"
    }
}
