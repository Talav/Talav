{
    "name": "talav/media-bundle",
    "description": "Symfony bundle to provide base media management.",
    "keywords": ["symfony", "bundle", "media"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Talav",
            "email": "support@talav.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "symfony/framework-bundle": "^5.1",
        "symfony/orm-pack": "^1.0",
        "liip/imagine-bundle": "^2.2",
        "oneup/flysystem-bundle": "^3.1",
        "vich/uploader-bundle": "^1.10",
        "talav/resource-bundle": "^1.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2",
        "symfony/phpunit-bridge": "^5.1",
        "symfony/thanks": "^1.2",
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "matthiasnoback/symfony-dependency-injection-test": "^4.1",
        "symfony/var-dumper": "^5.1",
        "talav/coding-standard": "@dev"
    },
    "autoload": {
        "psr-4": {
            "Talav\\MediaBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Talav\\MediaBundle\\Tests\\": "tests/",
            "AppBundle\\": "tests/Functional/src/AppBundle/"
        },
        "files": [
            "tests/Functional/app/AppKernel.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../../*/*"
        }
    ],
    "scripts": {
        "phpunit": "vendor/bin/phpunit",
        "check-cs": "vendor/bin/ecs check src --ansi",
        "fix-cs": "vendor/bin/ecs check src --fix --ansi"
    }
}
