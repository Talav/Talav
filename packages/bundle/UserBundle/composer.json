{
    "name": "talav/user-bundle",
    "description": "Symfony bundle to provide basic user functionality",
    "keywords": ["symfony", "bundle", "user", "php"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Talav",
            "email": "support@talav.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "symfony/framework-bundle": "^5.1",
        "symfony/config": "^5.1",
        "symfony/twig-bundle": "^5.1",
        "symfony/translation": "^5.1",
        "webmozart/assert": "^1.10",
        "symfony/orm-pack": "^2.1",
        "symfony/monolog-bundle": "^3.7",
        "symfony/validator": "^5.1",
        "egulias/email-validator": "^3.1",
        "symfony/form": "^5.1",
        "symfony/event-dispatcher": "^5.1",
        "symfony/mailer": "^5.1",
        "hwi/oauth-bundle": "^1.3",
        "php-http/guzzle6-adapter": "^2.0",
        "php-http/httplug-bundle": "^1.20",
        "talav/user": "@dev",
        "talav/resource-bundle": "@dev",
        "mark-gerarts/automapper-plus-bundle": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/phpunit-bridge": "^5.1",
        "symfony/var-dumper": "^5.1",
        "symfony/thanks": "^1.2",
        "symfony/console": "^5.1",
        "symfony/browser-kit": "^5.1",
        "symfony/css-selector": "^5.1",
        "matthiasnoback/symfony-dependency-injection-test": "^4.2",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "fakerphp/faker": "^1.9",
        "liip/test-fixtures-bundle": "^2.0.0-alpha5"
    },
    "autoload": {
        "psr-4": {
            "Talav\\UserBundle\\": "src/",
            "UserAppBundle\\": "tests/Functional/src/UserAppBundle/"
        },
        "exclude-from-classmap": [
            "/tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Talav\\UserBundle\\Tests\\": "tests/"
        },
        "files": [
            "Tests/Functional/app/UserAppKernel.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../../*/*"
        }
    ]
}
