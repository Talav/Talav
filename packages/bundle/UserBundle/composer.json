{
    "name": "talav/user-bundle",
    "description": "Symfony bundle to provide basic user functionality",
    "keywords": ["symfony", "bundle", "user", "php"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Talav",
            "email": "support@talav.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "symfony/framework-bundle": "^6.0",
        "symfony/config": "^6.0",
        "symfony/twig-bundle": "^6.0",
        "symfony/translation": "^6.0",
        "webmozart/assert": "^1.10",
        "symfony/orm-pack": "^2.2",
        "symfony/monolog-bundle": "^3.7",
        "symfony/validator": "^6.0",
        "egulias/email-validator": "^3.1",
        "symfony/form": "^6.0",
        "symfony/event-dispatcher": "^6.0",
        "symfony/mailer": "^6.0",
        "symfony/console": "^6.0",
        "hwi/oauth-bundle": "2.0.0-BETA2",
        "php-http/guzzle7-adapter": "^1.0",
        "php-http/httplug-bundle": "^1.25",
        "talav/user": "@dev",
        "talav/resource-bundle": "@dev",
        "mark-gerarts/automapper-plus-bundle": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/phpunit-bridge": "^6.0",
        "symfony/var-dumper": "^6.0",
        "symfony/thanks": "^1.2",
        "symfony/browser-kit": "^6.0",
        "symfony/css-selector": "^6.0",
        "matthiasnoback/symfony-dependency-injection-test": "^4.3",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "fakerphp/faker": "^1.19",
        "liip/test-fixtures-bundle": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "Talav\\UserBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Talav\\UserBundle\\Tests\\": "tests/",
            "UserAppBundle\\": "tests/TestApp/src/UserAppBundle/"
        },
        "files": [
            "tests/TestApp/app/UserAppKernel.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../../*/*"
        }
    ],
    "config": {
        "allow-plugins": {
            "symfony/thanks": false
        }
    }
}
