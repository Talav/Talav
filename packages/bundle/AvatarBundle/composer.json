{
    "name": "talav/avatar-bundle",
    "description": "Symfony bundle to provide avatar functionality for users",
    "keywords": ["symfony", "bundle", "user", "php"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Talav",
            "email": "support@talav.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "symfony/framework-bundle": "^6.0",
        "symfony/config": "^6.0",
        "symfony/twig-bundle": "^6.0",
        "symfony/translation": "^6.0",
        "webmozart/assert": "^1.10",
        "symfony/orm-pack": "^2.2",
        "symfony/monolog-bundle": "^3.7",
        "symfony/form": "^6.0",
        "talav/user-bundle": "@dev",
        "talav/media-bundle": "@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/phpunit-bridge": "^6.0",
        "symfony/browser-kit": "^6.0",
        "symfony/var-dumper": "^6.0",
        "symfony/console": "^6.0",
        "symfony/css-selector": "^6.0",
        "matthiasnoback/symfony-dependency-injection-test": "^4.3",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "fakerphp/faker": "^1.19",
        "liip/test-fixtures-bundle": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "Talav\\AvatarBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Talav\\AvatarBundle\\Tests\\": "tests/",
            "AvatarAppBundle\\": "tests/TestApp/src/AvatarAppBundle/"
        },
        "files": [
            "tests/TestApp/app/AvatarAppKernel.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/mihaileu/OneupFlysystemBundle.git"
        },
        {
            "type": "path",
            "url": "../../*/*"
        }
    ]
}
